{
  "name": "react-component-generator-with-css-modules",
  "publisher": "SaiRev",
  "icon": "React_Code_Generator.png",
  "displayName": "React Component Generator with css Modules",
  "description": "Creates react file and a css module file and will write a boilerplate code in the js file",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-component-generator-with-css-modules.createComponent",
        "title": "Create Component"
      },
      {
        "command": "react-component-generator-with-css-modules.convertClassIntoClassName",
        "title": "Convert Class into ClassName"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaiRev0/React-Component-Generator-vsCode-Extension"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  }
}
