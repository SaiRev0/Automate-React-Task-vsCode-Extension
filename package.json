{
  "name": "automate-react-task",
  "publisher": "SaiRev",
  "icon": "React_Code_Generator.png",
  "displayName": "Automate React Task",
  "description": "Help automate many react component creation tasks",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "automate-react-task.createComponent",
        "title": "Create Component"
      },
      {
        "command": "automate-react-task.convertClassIntoClassName",
        "title": "Convert Class into ClassName"
      },
      {
        "command": "automate-react-task.convertHyphenToUnderscore",
        "title": "Convert Hyphen to Underscore"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaiRev0/Automate-React-Task-vsCode-Extension"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  }
}
